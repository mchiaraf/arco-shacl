ex:UrbanPlanningInstrumentShape
    a sh:NodeShape ;
    sh:targetClass a-cd:UrbanPlanningInstrument ;
	sh:property [              
		sh:path core:hasType ;     
		sh:class a-cd:UrbanPlanningInstrumentType ;
		sh:severity sh:Violation ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:message "La relazione dello strumento urbanistico-territoriale con il suo tipo Ã¨ obbligatoria: core:hasType deve essere materializzata"
		] ;
	# NON OBBL ANAGRAFICA
	sh:property [              
		sh:path a-cd:definesEligibleIntervention ;     
		sh:nodeKind sh:Literal ;
		] ;
	sh:property [              
		sh:path a-lite:hasIssuingAgency ;     
		sh:class l0:Agent ;
		] ;
	sh:property [              
		sh:path sm:URL ;     
		sh:nodeKind xsd:anyURI ;
		] ;
	sh:property [              
		sh:path core:note ;     
		sh:nodeKind sh:Literal ;
		] .