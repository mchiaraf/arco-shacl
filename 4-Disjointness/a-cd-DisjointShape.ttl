### CONTEXT DESCRIPTION

ex:DocumentationTypeDisjointShape
    a sh:NodeShape ;
    sh:targetSubjectsOf rdf:type ; # soggetti con un tipo
    sh:filterShape [     
        sh:property [
            sh:path rdf:type ;
            sh:in (
                a-cd:DocumentationType
                a-cd:BibliographyType
            ) ;
        ] ;
    ] ;
	sh:message "Un individuo della classe a-cd:DocumentationType non può essere anche di tipo a-cd:BibliographyType" ;
    sh:severity sh:Violation ;
	sh:xone ( 	# solo una classe tra le seguenti
        [ sh:class a-cd:DocumentationType ]
        [ sh:class a-cd:BibliographyType ]
    ) .


