### NATURAL SPECIMEN DESCRIPTION 

ex:SpecimenHarvestingAccessionDisjointShape
    a sh:NodeShape ;
    sh:targetSubjectsOf rdf:type ; # soggetti con un tipo
    sh:filterShape [     # restrizione solo se il tipo è uno dei seguenti tipi
        sh:property [
            sh:path rdf:type ;
            sh:in (
                a-spe:Accession
                a-spe:SpecimenHarvesting
            ) ;
        ] ;
    ] ;
    sh:message "Un individuo può essere di una sola tipologia tra a-spe:Accession, a-spe:SpecimenHarvesting." ;
    sh:severity sh:Violation ;
    sh:xone ( 	# solo una classe tra le seguenti
        [ sh:class a-spe:Accession ]
        [ sh:class a-spe:SpecimenHarvesting ]
    ) .

