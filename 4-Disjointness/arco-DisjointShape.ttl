### ARCO
ex:MovableDisjointShape
    a sh:NodeShape ;
    sh:targetClass arco:MovableCulturalProperty ;
	sh:message "Un bene mobile non può essere anche un bene immobile" ;
    sh:severity sh:Violation ;
	sh:not [ 
		sh:class arco:ImmovableCulturalProperty 
		] .

ex:ImmovableDisjointShape
    a sh:NodeShape ;
    sh:targetClass arco:ImmovableCulturalProperty ;
	sh:message "Un bene immobile non può essere anche un bene mobile" ;
    sh:severity sh:Violation ;
	sh:not [ 
		sh:class arco:MovableCulturalProperty 
		] .

ex:TangibleDisjointShape
    a sh:NodeShape ;
    sh:targetClass arco:TangibleCulturalProperty ;
	sh:message "Un bene materiale non può essere anche un bene immateriale" ;
    sh:severity sh:Violation ;
	sh:not [ 
		sh:class arco:IntangibleCulturalProperty 
		] .

ex:IntangibleDisjointShape
    a sh:NodeShape ;
    sh:targetClass arco:IntangibleCulturalProperty ;
	sh:message "Un bene immateriale non può essere anche un bene materiale" ;
    sh:severity sh:Violation ;
	sh:not [ 
		sh:class arco:TangibleCulturalProperty 
		] .
