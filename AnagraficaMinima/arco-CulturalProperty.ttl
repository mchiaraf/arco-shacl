            [ sh:class arco:PhotographicHeritage ]
            [ sh:class arco:ScientificOrTechnologicalHeritage ]
            [ sh:class cis:CulturalEntity ]
        ) ;
    ] ;
    sh:or (
		[
			sh:path a-lite:isLocatedIn ;     
			sh:or (
            [ sh:class arco:CulturalProperty ]
            [ sh:class arco:ComplexCulturalProperty ]
            [ sh:class arco:DemoEthnoAnthropologicalHeritage ]
            [ sh:class arco:IntangibleCulturalProperty ]
            [ sh:class arco:TangibleCulturalProperty ]
            [ sh:class arco:ArchaeologicalProperty ]
            [ sh:class arco:ArchaeologicalMaterial ]
            [ sh:class arco:ImmovableCulturalProperty ]
            [ sh:class arco:ArchitecturalOrLandscapeHeritage ]
            [ sh:class arco:MovableCulturalProperty ]
            [ sh:class arco:HistoricOrArtisticProperty ]
            [ sh:class arco:ArchaeologicalProperty ]
            [ sh:class arco:MusicHeritage ]
            [ sh:class arco:NaturalHeritage ]
            [ sh:class arco:NumismaticProperty ]
            [ sh:class arco:PhotographicHeritage ]
            [ sh:class arco:ScientificOrTechnologicalHeritage ]
            [ sh:class cis:CulturalEntity ]
            ) ;
			sh:severity sh:Warning ;
        	sh:message "La relazione a-lite:isLocatedIn deve collegare necessariamente due entità culturali"
		]
        [
			sh:path a-lite:hasPreviousLocation ;     
			sh:or (
            [ sh:class arco:CulturalProperty ]
            [ sh:class arco:ComplexCulturalProperty ]
            [ sh:class arco:DemoEthnoAnthropologicalHeritage ]
            [ sh:class arco:IntangibleCulturalProperty ]
            [ sh:class arco:TangibleCulturalProperty ]
            [ sh:class arco:ArchaeologicalProperty ]
            [ sh:class arco:ArchaeologicalMaterial ]
            [ sh:class arco:ImmovableCulturalProperty ]
            [ sh:class arco:ArchitecturalOrLandscapeHeritage ]
            [ sh:class arco:MovableCulturalProperty ]
            [ sh:class arco:HistoricOrArtisticProperty ]
            [ sh:class arco:ArchaeologicalProperty ]
            [ sh:class arco:MusicHeritage ]
            [ sh:class arco:NaturalHeritage ]
            [ sh:class arco:NumismaticProperty ]
            [ sh:class arco:PhotographicHeritage ]
            [ sh:class arco:ScientificOrTechnologicalHeritage ]
            [ sh:class cis:CulturalEntity ]
            ) ;
			sh:severity sh:Warning ;
        	sh:message "La relazione a-lite:hasPreviousLocation deve collegare necessariamente due entità culturali"
		]
		[
			sh:path a-lite:isInvolvedIn ;     
			sh:or (
            [ sh:class arco:CulturalProperty ]
            [ sh:class arco:ComplexCulturalProperty ]
            [ sh:class arco:DemoEthnoAnthropologicalHeritage ]
            [ sh:class arco:IntangibleCulturalProperty ]
            [ sh:class arco:TangibleCulturalProperty ]
            [ sh:class arco:ArchaeologicalProperty ]
            [ sh:class arco:ArchaeologicalMaterial ]
            [ sh:class arco:ImmovableCulturalProperty ]
            [ sh:class arco:ArchitecturalOrLandscapeHeritage ]
            [ sh:class arco:MovableCulturalProperty ]
            [ sh:class arco:HistoricOrArtisticProperty ]
            [ sh:class arco:ArchaeologicalProperty ]
            [ sh:class arco:MusicHeritage ]
            [ sh:class arco:NaturalHeritage ]
            [ sh:class arco:NumismaticProperty ]
            [ sh:class arco:PhotographicHeritage ]
            [ sh:class arco:ScientificOrTechnologicalHeritage ]
            [ sh:class cis:CulturalEntity ]
            ) ;
			sh:severity sh:Warning ;
        	sh:message "La relazione a-lite:isInvolvedIn deve collegare necessariamente due entità culturali"
		]
        [
			sh:path a-lite:hasCreationLocation ;     
			sh:or (
            [ sh:class arco:CulturalProperty ]
            [ sh:class arco:ComplexCulturalProperty ]
            [ sh:class arco:DemoEthnoAnthropologicalHeritage ]
            [ sh:class arco:IntangibleCulturalProperty ]
            [ sh:class arco:TangibleCulturalProperty ]
            [ sh:class arco:ArchaeologicalProperty ]
            [ sh:class arco:ArchaeologicalMaterial ]
            [ sh:class arco:ImmovableCulturalProperty ]
            [ sh:class arco:ArchitecturalOrLandscapeHeritage ]
            [ sh:class arco:MovableCulturalProperty ]
            [ sh:class arco:HistoricOrArtisticProperty ]
            [ sh:class arco:ArchaeologicalProperty ]
            [ sh:class arco:MusicHeritage ]
            [ sh:class arco:NaturalHeritage ]
            [ sh:class arco:NumismaticProperty ]
            [ sh:class arco:PhotographicHeritage ]
            [ sh:class arco:ScientificOrTechnologicalHeritage ]
            [ sh:class cis:CulturalEntity ]
            ) ;
			sh:severity sh:Warning ;
        	sh:message "La relazione a-lite:hasCreationLocation deve collegare necessariamente due entità culturali"
		]
        [
			sh:path a-lite:isReusedBy ;     
			sh:or (
            [ sh:class arco:CulturalProperty ]
            [ sh:class arco:ComplexCulturalProperty ]
            [ sh:class arco:DemoEthnoAnthropologicalHeritage ]
            [ sh:class arco:IntangibleCulturalProperty ]
            [ sh:class arco:TangibleCulturalProperty ]
            [ sh:class arco:ArchaeologicalProperty ]
