            [ sh:class arco:NaturalHeritage ]
            [ sh:class arco:NumismaticProperty ]
            [ sh:class arco:PhotographicHeritage ]
            [ sh:class arco:ScientificOrTechnologicalHeritage ]
            [ sh:class cis:CulturalEntity ]
            ) ;
			sh:severity sh:Warning ;
        	sh:message "La relazione a-lite:hasUrbanPlanningEnvironmentalRelationWith deve collegare necessariamente due entità culturali"
		]
        [
			sh:path a-lite:hasFindingLocation ;     
			sh:or (
            [ sh:class arco:CulturalProperty ]
